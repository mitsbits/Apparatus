@model object
@{
    ViewData["Title"] = "Zoo";
    var props = Model.PropertyNames();
}


<div class="row">
    <form asp-controller="Zoo" asp-action="Index" method="post" class="form-horizontal" role="form">
        @foreach(var prop in  props)
        {
        @Html.Editor(prop)
        }

        <div class="form-group">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
</div> 
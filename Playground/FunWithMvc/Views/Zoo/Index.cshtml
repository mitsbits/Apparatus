@{
    ViewData["Title"] = "Zoo";
}
@model FunWithMvc.Models.Zoo.Dog

<div class="row">
    <form asp-controller="Zoo" asp-action="Index" method="post" class="form-horizontal" role="form">
        @Html.EditorFor(m => m.Id)
        @Html.EditorFor(m => m.Name)
        @Html.EditorFor(m => m.Spieces)
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
</div> 
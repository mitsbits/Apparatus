{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Borg": {
    "Framework": {
      "SQLServer": {
        "Broadcast": {
          "SqlBroadcastBus": {
            "SqlConnectionString": "Server=.;Database=Borg;Trusted_Connection=True;MultipleActiveResultSets=true;",
            "SubcriberName": "Web Subscriber",
            "QueuesToListen": [ "queue1", "queue2", "queue3" ],
            "PollingIntervalInSeconds": 5
          }
        }
      }
    }
  }
}
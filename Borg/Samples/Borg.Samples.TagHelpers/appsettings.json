{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Borg": {
    "Framework": {
      "MVC": {
        "Features": {
          "HtmlPager": {
            "OutputTagElement": "ul",
            "OutputItemTagElement": "li",
            "ItemSliceAndTotalFormat": "{0} to {1} of {2}",
            "PageCountAndLocationFormat": "{0} of {1}",
            "NextDisplay": ">",
            "LastDisplay": ">>",
            "PreviousDisplay": "<",
            "FirstDisplay": "<<",
            "PageDisplayFormat": "{0}",
            "PageVariableName": "p",
            "ElementClass": "erer",
            "ItemClass": "page_item",
            "CurrentClass": "active",
            "UnavailableClass": "disabled",
            "ArrowClass": "",
            "Ellipses": "xxx"
          }
        }
      }
    }
  }
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Borg": {
    "Framework": {
      "MVC": {
        "Features": {
          "HtmlPager": {
            "OutputTagElement": "ul",
            "OutputItemTagElement": "li",
            "ItemSliceAndTotalFormat": "{0} to {1} of {2}",
            "PageCountAndLocationFormat": "{0} of {1}",
            "NextDisplay": "\u003E",
            "LastDisplay": "\u003E\u003E",
            "PreviousDisplay": "\u003C",
            "FirstDisplay": "\u003C\u003C",
            "PageDisplayFormat": "{0}",
            "PageVariableName": "p",
            "LinkClass": "page-link",
            "ItemClass": "page-item",
            "ElementClass": "pagination",
            "CurrentClass": "active",
            "UnavailableClass": "disabled",
            "ArrowClass": "",
            "Ellipses": "...",
            "Behaviour": {
              "ChunkCount": 5,
              "DisplayEllipsesWhenNotShowingAllPageNumbers": true,
              "DisplayLinkToNextPage": true,
              "DisplayLinkToPreviousPage": true,
              "DisplayLinkToLastPage": true,
              "DisplayLinkToFirstPage": true,
              "DisplayPageCountAndCurrentLocation": true,
              "MaximumPageNumbersToDisplay": 10,
              "DisplayLinkToIndividualPages": true,
              "DisplayItemSliceAndTotal": true,
              "PagerInChunks": true,
              "PageVariable": "p",
              "RowsVariable": "r",
              "ActiveItemClassOperation": "CurrentClass"
            }
          }
        }
      }
    }
  }
}